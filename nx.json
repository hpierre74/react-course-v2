      }
    }
  },
  "projects": {
    "api": {
      "tags": []
    },
    "course-hints": {
      "tags": []
    },
    "exercise-1": {
      "tags": []
    },
    "exercise-1-e2e": {
      "tags": [],
      "implicitDependencies": ["exercise-1"]
    },
    "exercise-2": {
      "tags": []
    },
    "exercise-2-e2e": {
      "tags": [],
      "implicitDependencies": ["exercise-2"]
    },
    "exercise-3": {
      "tags": []
    },
    "exercise-3-e2e": {
      "tags": [],
      "implicitDependencies": ["exercise-3"]
    },
    "react-course-demo": {
      "tags": []
    },
    "react-course-demo-e2e": {
      "tags": [],
      "implicitDependencies": ["react-course-demo"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
